#include<stdio.h>
int main(){
int A[4][4], transpose[4][4];
printf("enter the elements of a 4*4 matrix; \n");
for(int i=0; i<4; i++){
for(int j=0; j<4; j++){
scanf("%d", &A[i][j]);
}
}
 for(int i=0; i<4; i++){
 for(int j=0; j<4; j++){
 transpose[i][j]= A[i][j];
 }
 }
  printf("transpose of the matrix: \n");
  for(int i=0; i<4; i++){
  for(int j=0; j<4; j++){
  printf("%d", transpose[i][j]);
  }
  printf("\n");
  }
  return 0;


